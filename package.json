{
  "name": "@iml/old-gui",
  "version": "2.1.0",
  "description": "",
  "scripts": {
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/old-gui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jasmine-core": "2.6.3",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-script-launcher": "1.0.0"
  },
  "jshintConfig": {
    "predef": [
      "angular",
      "_",
      "module",
      "inject",
      "jasmine",
      "spyOn",
      "it",
      "console",
      "describe",
      "expect",
      "beforeEach",
      "afterEach",
      "mock",
      "Mock",
      "Timecop",
      "waits",
      "waitsFor",
      "runs"
    ],
    "bitwise": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 2,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxcomplexity": 6,
    "maxlen": 120,
    "eqnull": true,
    "multistr": true,
    "browser": true,
    "jquery": true
  }
}
